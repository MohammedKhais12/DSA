class Solution {
public:
    vector<int> decompressRLElist(vector<int>& nums) {
        vector <int> result;
        int val;
        int freq;

        for(int i=0; i<nums.size(); i+2){
            freq = nums[i];
            val = nums[i+1];
            for(int j=0; j<freq; j++){
                result.push_back(val);
            }
        }
        return result;
    }
};
