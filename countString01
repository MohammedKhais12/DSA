// User function template for C++
class Solution {
  public:
    // #define ll long long
    int countStrings(int n) {
        // code here
        if(n==1)return 2;
        if(n==2)return 3;
        int a = 2;
        int b = 3;
        int c = a+b;
        for(int i=3; i<n; i++){
            a = b; 
            b = c;
            c = a+b;
 
        }
        return c;
    }
};
