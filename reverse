class Solution {
  public:
    string reverse(string str) {
        // code here.
        int i = 0;
        int j = str.size()-1;
        while(i<j){
            if(!isalpha(str[i])){
                i++;
            }
            if(!isalpha(str[j])){
                j--;
            }
            if(isalpha(str[i]) && isalpha(str[j]) ){
                swap(str[i], str[j]);
                i++;
                j--;
            }
        }
        return str;
    }
};
