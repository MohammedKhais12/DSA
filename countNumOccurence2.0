class Solution {
  public:
    int num(int k, vector<int> &arr) {
        int count = 0;

        for(int num : arr) {
            num = abs(num);

            if(num == 0 && k == 0) {
                count++;
            }

            while(num > 0) {
                if(num % 10 == k) {
                    count++;
                }
                num /= 10;
            }
        }

        return count;
    }
};
