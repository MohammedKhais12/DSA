class Solution {
  public:
    vector<int> findUnion(vector<int> &a, vector<int> &b) {
        // code here
        unordered_map<int, int> freq;
        vector<int> vec;
        
        for(int i : a){
            freq[i]++;
        }
        for(int j : b){
            freq[j]++;
        }
        
        for(auto &p : freq){
            vec.push_back(p.first);
        }
        sort(vec.begin(), vec.end());
        return vec;
    }
};
