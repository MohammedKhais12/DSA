class Solution {
  public:
    vector<int> kLargest(vector<int>& arr, int k) {
        // Your code here
        sort(arr.begin(), arr.end());
        vector<int> sun;
        int n = arr.size()-1;
        
        for(int i=0; i<k; i++){
            sun.push_back(arr[n-i]);
        }
        return sun;
    }
};
