class Solution {
  public:
    vector<int> findDuplicates(vector<int>& arr) {
        // code here
        unordered_map <int, int> un;
        vector <int> vec;
        
        for(int i =0; i<arr.size(); i++){
            int num = arr[i];
            if(un.find(num)!=un.end()){
                vec.push_back(num);
            }
            un[num]++;
        }
        return vec;
    }
};
