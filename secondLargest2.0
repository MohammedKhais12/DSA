class Solution {
  public:
    int getSecondLargest(vector<int> &arr) {
        // code here
        sort(arr.begin(), arr.end(), greater<int>());
        int maxi = *max_element(arr.begin(), arr.end());
        
        for(int i=1; i<arr.size(); i++){
            if(maxi>arr[i]){
                return arr[i];
            }
        }
        return -1;
    }
};
