class Solution {
public:
    bool isPalindrome(int x) {
        if(x<0){
            return false;
        }
        long long num = x;
        long long newX = 0;
        int digit;
        while(num!=0){
            digit = num%10;
            newX *= 10;
            newX += digit;
            num /= 10;
        }
        if(x==newX){
            return true;
        }
        return false;
    }
};
