class Solution {
  public:
    int countFreq(vector<int>& arr, int target) {
        // code here
        unordered_map <int, int> map;
        for(int i =0; i<arr.size(); i++){
            map[arr[i]]++;
        }
        for(auto i : map){
            if(target == i.first){
            return i.second;                
            }
        }
        return 0;
    }
};
