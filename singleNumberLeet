class Solution {
public:
    vector<int> singleNumber(vector<int>& nums) {
        int n = nums.size();
        unordered_map<int, int> freq;
        vector<int> vec;

        for(int n : nums){
            freq[n]++;
        }
        for(int n: nums){
            if(freq[n]==1){
                vec.push_back(n);
            }
        }
        return vec;
    }
};
