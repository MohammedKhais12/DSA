class Solution {
public:
    int findNumbers(vector<int>& nums) {
        bool valid = false;
        int count;
        int numbers = 0;

        for(int i=0; i<nums.size(); i++){
            int x = nums[i];
            count = 1;
            while(x>9){
                x = x/10;
                count++;
            }
            if(count%2==0){
                numbers++;
            }
        }
        return numbers;
    }
};
